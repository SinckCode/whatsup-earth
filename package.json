{
  "name": "whatsup-earth",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "start": "concurrently -k \"npm:dev\" \"npm:electron\"",
    "build:renderer": "vite build",
    "build:electron": "vite build --mode production --config vite.config.js",
    "build": "npm run build:renderer && npm run build:electron && electron-builder",
    "lint": "eslint src --ext .js,.jsx"
  },
  "build": {
    "appId": "com.onesto.whatsupearth",
    "files": [
      "dist/**",
      "dist-electron/**",
      "electron/**"
    ],
    "directories": {
      "buildResources": "build"
    }
  },
  "dependencies": {
    "@react-three/fiber": "^9.3.0",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "lucide-react": "^0.548.0",
    "maplibre-gl": "^5.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-globe.gl": "^2.36.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.3.0",
    "swiper": "^11.2.10",
    "three": "^0.179.1",
    "zod": "^4.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "vite": "^7.1.3",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.4"
  }
}
